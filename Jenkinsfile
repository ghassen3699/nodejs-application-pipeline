pipeline{
    agent any
    stages{
        stage('build'){
            steps{
                script{
                    echo "build project"
                }
            }
        }

        stage('test'){
            steps{
                script{
                    echo "test project"
                }
            }
        }

        stage('create image'){
            steps{
                script{
                    echo "create image"
                }
            }
        }

        stage('deploy'){
            steps{
                script{
                    echo "deploy project"
                }
            }
        }
    }
}